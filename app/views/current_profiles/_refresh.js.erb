(function (document) {
  let last = document.getElementById("network_links").getAttribute("data-network-links-last");

  document.getElementById("dashboard").
    dispatchEvent(
      new CustomEvent(
        "component:refresh", { 
          detail: {
            target: "networkLinks",
            data: "<%= j render "application/network_links" %>"
          }
        }
      )
    );

    document.getElementById("network_links").setAttribute("data-network-links-last", last);
})(document);

<%= render "messages/reset" %>
